{
  "name": "interact",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev --workspaces",
    "dev:showcase": "vite --open",
    "build": "npm run build --workspaces",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "test": "npm run test --workspace packages/interact --workspace packages/motion",
    "dev:docs": "npm run dev --workspace apps/docs",
    "dev:demo": "npm run dev --workspace apps/demo",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/interact.git"
  },
  "keywords": [
    "animation",
    "javascript",
    "css",
    "waapi"
  ],
  "author": {
    "name": "wow!Team",
    "email": "wow-dev@wix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix-incubator/interact/issues"
  },
  "homepage": "https://github.com/wix-incubator/interact#readme",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
